<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.goodline.starsandplanets" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.ActivitySpaceBodyList"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ActivitySpaceBodyDetail"
            android:label="@string/title_spacebody_detail"
            android:parentActivityName=".activity.ActivitySpaceBodyList" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activity.ActivitySpaceBodyList" />
        </activity>
    </application>

</manifest>
